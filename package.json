{
  "name": "css-transform-matrix",
  "version": "0.0.2",
  "description": "You can easily customize what you want with the CssMatrix",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "example.gif"
  ],
  "scripts": {
    "dev": "rollup -c config/rollup.config.js --bundleConfigAsCjs -w --environment NODE_ENV:dev",
